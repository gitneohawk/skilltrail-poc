{
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/interview",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/login",
      "rewrite": "/login.html"
    },
    {
      "route": "/",
      "rewrite": "/index.html"
    },
    {
      "route": "/chat",
      "rewrite": "/chat.html"
    }
  ],
  "auth": {
    "identityProviders": {
      "github": {
        "enabled": true
      }
    }
  },
  "navigationFallback": {
    "rewrite": "/index.html"
  }
}