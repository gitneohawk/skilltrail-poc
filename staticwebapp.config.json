{
  "auth": {
    "identityProviders": {
      "github": {
        "enabled": true
      },
      "google": {
        "registration": {
          "clientId": "532547962834-9f15h02cbad4ob336o295mn0qkptr3s8.apps.googleusercontent.com",
          "clientSecretSettingName": "GOOGLE_CLIENT_SECRET"
        },
        "enabled": true
      }
    }
  },
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/interview",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/login",
      "rewrite": "/login.html"
    },
    {
      "route": "/",
      "rewrite": "/index.html"
    },
    {
      "route": "/chat",
      "rewrite": "/chat.html"
    }
    {
  "route": "/.auth/login/done",
  "rewrite": "/index.html"
}
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  }
}